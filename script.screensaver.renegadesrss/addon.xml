<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.screensaver.renegadesrss" name="Renegades RSS screensaver" version="0.0.1" provider-name="Cypher">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.feedparser" version="5.1.3"/>
        <import addon="script.module.requests" version="2.3.0"/>
    </requires>
    <extension point="xbmc.ui.screensaver" library="screensaver.py" />
	<extension point="xbmc.python.script" library="screensaver.py" />
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <language/>
        <website></website>
        <source></source>
        <forum></forum>
        <email></email>
        <summary lang="en">Renegades RSS screensaver</summary>
        <description lang="en">Renegades RSS screensaver will shows you the Latest Football Games and what Channels they are on... other feeds can be added from within the addon settings</description>
    </extension>
</addon>
